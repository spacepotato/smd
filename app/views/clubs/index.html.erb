<h1>Listing Clubs</h1>

<table border="4" cellpadding="10">
  <thead>
    <tr>
      <th>Name</th>
      <th>Weblink</th>
      <th>Registration Number</th>
      <!--<th colspan="3"></th>-->
    </tr>
  </thead>

  <tbody>
    <% @clubs.each do |club| %>
    <tr>
      <td><%= club.name %></td>
      <td><%= club.webLink %></td>
      <td><%= club.registrationNumber %></td>
      <td><%= link_to 'Show', club %></td>
      <td></td>
      <td><%= link_to 'Destroy', club, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>

    <% end %>
  </tbody>
</table>


<br>

<%= link_to "New Club" , new_club_path %>



